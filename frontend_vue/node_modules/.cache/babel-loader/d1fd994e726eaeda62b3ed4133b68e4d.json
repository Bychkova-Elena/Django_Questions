CD,GADH,CACO,cADP,EAEG,IAFH,CAEQ,UAAA,QAAO,EAAK;AAChB,QAAA,KAAI,CAAC,IAAL,GAAY,QAAQ,CAAC,IAArB;AACD,OAJH,EAKG,KALH,CAKS,UAAA,KAAI,EAAK;AACd,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAPH;AAQD,KAVQ;AAWT,IAAA,GAXS,eAWL,EAXK,EAWD;AAAA;;AACN,MAAA,KAAI,CACH,MADD,8BAC8B,EAD9B,GAEC,IAFD,CAEM,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,OAAL;;AACI,QAAA,KAAK,CAAC;AACJ,UAAA,OAAO,EAAE,0BADL;AAEJ,UAAA,IAAI,EAAE,YAFF;AAGJ,UAAA,WAAW,EAAE,IAHT;AAIJ,UAAA,YAAY,EAAE,IAJV;AAKJ,UAAA,QAAQ,EAAE,IALN;AAMJ,UAAA,QAAQ,EAAE;AANN,SAAD,CAAL;AAQD,OAZL;AAaD;AAzBQ,GAZE;AAyCX,EAAA,QAAQ,EAAC;AACP,IAAA,aADO,2BACQ;AAAA;;AACb,aAAO,KAAK,IAAL,CAAU,MAAV,CAAiB,UAAA,IAAG,EAAK;AAC9B,eAAO,IAAI,CAAC,KAAL,CAAW,WAAX,GAAyB,QAAzB,CAAkC,MAAI,CAAC,MAAL,CAAY,WAAZ,EAAlC,CAAP;AACD,OAFM,CAAP;AAGF;AALO;AAzCE,CAAf","sourcesContent":["<template>\r\n        <section class=\"section\">\r\n      <div class=\"container is-fluid\">\r\n        <div class=\"mt-3\">\r\n          <div class=\"is-flex is-justify-content-space-between\">\r\n          <h1 class=\"title mb-6\">Новости</h1>\r\n          <router-link to=\"/add-news\" class=\"button is-warning\">Добавить новость</router-link>\r\n          </div>\r\n          <div class=\"control\">\r\n          <input type=\"search\" class=\"input is-warning mb-6\" v-model=\"search\" placeholder=\"Поиск по заголовку\">\r\n          </div>\r\n            <div v-for=\"n in searchHandler\" :key=\"n.id\">\r\n              <div class=\"card mb-3\">\r\n  <header class=\"card-header\">\r\n    <p class=\"card-header-title\">\r\n      {{n.title}}\r\n    </p>\r\n  </header>\r\n  <div class=\"card-content\">\r\n    <div class=\"content\">\r\n     <p>{{n.body}}</p>\r\n    </div>\r\n  </div>\r\n  </div>\r\n\r\n\r\n  <div class=\"field is-grouped mb-6\">\r\n  <p class=\"control\">\r\n    <router-link :to=\"{name: 'EditNews', params: {id: n.id, title:n.title, body:n.body}}\" class=\"button is-primary\">Редактировать новость</router-link>\r\n  </p>\r\n  <p class=\"control\">\r\n    <button @click=\"del(n.id)\" class=\"button is-danger\">\r\n      Удалить новость\r\n    </button>\r\n  </p>\r\n</div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n    </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport { toast } from \"bulma-toast\";\r\n\r\nexport default {\r\n    name: 'News',\r\n    data() {\r\n    return {\r\n      news: [],\r\n      search:'',\r\n    };\r\n  },\r\n    mounted() {\r\n        document.title = 'Новости | DjangoQuestions',\r\n        this.getNews()\r\n    },\r\n    methods: {\r\n       getNews() {\r\n      axios\r\n        .get(\"api/v1/news/\")\r\n        .then(response => {\r\n          this.news = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    del(id) {\r\n      axios\r\n      .delete(`api/v1/news/delete/${id}`)\r\n      .then((response) => {\r\n        this.getNews();\r\n            toast({\r\n              message: \"Новость успешно удалена!\",\r\n              type: \"is-success\",\r\n              dismissible: true,\r\n              pauseOnHover: true,\r\n              duration: 2000,\r\n              position: \"bottom-right\",\r\n            });\r\n          })\r\n    },\r\n        \r\n    },\r\n\r\n    computed:{\r\n      searchHandler(){\r\n        return this.news.filter(elem => {\r\n          return elem.title.toLowerCase().includes(this.search.toLowerCase());\r\n        })\r\n      }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scope>\r\n\r\n\r\n</style>"],"sourceRoot":""}]}